{
  "name": "OpenChordCharts-sample-data",
  "version": "0.0.0",
  "scripts": {
    "build:prod": "NODE_ENV=production webpack",
    "clean": "rm -rf dist",
    "deploy": "git branch -D gh-pages; git worktree add -b tmp gh-pages; cd gh-pages; git checkout --orphan gh-pages; git rm -rf .; cp ../dist/* .; git add --all; git commit -m 'Static page'; git push -f origin gh-pages; cd ..; rm -rf gh-pages; git worktree prune; git branch -d tmp",
    "dev": "webpack-dev-server --colors --port 3010",
    "lint": "eslint src test",
    "predeploy": "npm run clean; npm run build:prod",
    "test": "mocha --compilers js:babel-core/register --recursive"
  },
  "devDependencies": {
    "babel": "^6.1.18",
    "babel-core": "^6.2.1",
    "babel-eslint": "^5.0.0-beta4",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babel-preset-stage-0": "^6.1.18",
    "deep-freeze": "0.0.1",
    "eslint": "^1.10.3",
    "eslint-plugin-react": "^3.11.2",
    "expect": "^1.13.0",
    "html-webpack-plugin": "^1.7.0",
    "json-loader": "^0.5.4",
    "mocha": "^2.3.4",
    "redux-devtools": "^3.0.0-beta-3",
    "redux-devtools-dock-monitor": "^1.0.0-beta-3",
    "redux-devtools-log-monitor": "^1.0.0-beta-3",
    "webpack-dev-server": "^1.14.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.2.0",
    "deep-equal": "^1.0.1",
    "react": "^0.14.3",
    "react-clipboard.js": "^0.1.5",
    "react-dom": "^0.14.3",
    "react-hotkeys": "^0.7.1",
    "react-redux": "^4.0.0",
    "redux": "^3.0.4",
    "redux-undo": "^0.5.0",
    "reselect": "^2.0.1",
    "transduce": "^0.9.0"
  }
}
