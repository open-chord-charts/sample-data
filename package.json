{
  "name": "openchordcharts-sample-data",
  "version": "0.0.0",
  "scripts": {
    "deploy": "npm run build; git branch -D gh-pages; git worktree add -b tmp gh-pages; cd gh-pages; git checkout --orphan gh-pages; git rm -rf .; cp ../dist/* .; git add --all; git commit -m 'Static page'; git push -f origin gh-pages; cd ..; rm -rf gh-pages; git worktree prune; git branch -d tmp",
    "lint": "standard",
    "start": "kotatsu serve --babel --config webpack.config.babel.js --progress src/index.js"
  },
  "dependencies": {
    "mobx": "^2.1.6",
    "mobx-react": "^3.1.0",
    "ramda": "^0.21.0",
    "react": "^15.0.2",
    "react-clipboard.js": "^0.2.3",
    "react-dom": "^15.0.2",
    "react-hotkeys": "^0.8.0"
  },
  "devDependencies": {
    "babel-eslint": "^6.0.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "kotatsu": "^0.13.0",
    "mobx-react-devtools": "^4.0.5",
    "standard": "^7.0.1",
    "webpack": "^1.13.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react",
      "react-hmre"
    ],
    "plugins": [
      "transform-decorators-legacy"
    ]
  },
  "standard": {
    "parser": "babel-eslint"
  }
}
